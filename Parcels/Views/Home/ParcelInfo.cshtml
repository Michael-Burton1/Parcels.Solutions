@using EpicodusShipping.Models;

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Parcel Info</title>
  </head>
  <body>
    <ul>
      @foreach (Parcel parcel in Model) {
      <li>
        <p>Weight: @parcel.Weight</p>
        <p>Height: @parcel.Dimensions[0]</p>
        <p>Width: @parcel.Dimensions[1]</p>
        <p>Depth: @parcel.Dimensions[2]</p>
      </li>
      }
    </ul>
  </body>
</html>
